{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": 
  [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": 
      {
        "files": 
        [
          "/index.html",
          "/keycloak.html",
          "/manifest.webmanifest",
          "/dist/*.css",
          "/dist/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": 
      {
        "files": 
        [
          "/assets/**"
        ]
      }
    },
    {
      "name": "markdowns",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": 
      {
        "files": 
        [
          "/md/**"
        ]
      }
    }
  ]
}
